<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ctproject.caridosenug">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- SplashActivity -->
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />"
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- MainActivity -->
        <activity
            android:name=".MenuActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name" >
        </activity>
        <!-- ChildActivity -->
        <activity
            android:name=".DosenActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_list">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MenuActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".DosenDetailActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_profil">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DosenActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".InformasiActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_informasi">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DosenActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".TambahDosenActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_tambah">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DosenActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".BantuanActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_bantuan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InformasiActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".BantuanDosenActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_bantuan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DosenActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".TentangAplikasiActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_taplikasi">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InformasiActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".TentangKamiActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_tkami">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InformasiActivity"
                >
            </meta-data>
        </activity>
        <activity
            android:name=".HakCiptaActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name_hakcipta">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InformasiActivity"
                >
            </meta-data>
        </activity>

        <activity
            android:name=".TutorPertamaActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>
